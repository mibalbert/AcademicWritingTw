<title>Academic Writing - Home</title>

{{#if success}}
  <div class='alert alert-success alert-dismissible fade show' id="alert" role='alert'>
    <strong>{{success}}</strong>
    <button
      type='button'
      class='btn-close'
      data-bs-dismiss='alert'
      aria-label='Close'
    ></button>
  </div>
{{/if}}
{{#if registered}}
  <div class='alert alert-success alert-dismissible fade show' id="alert" role='alert'>
    <strong>{{registered}}</strong>
    New account registered
    <button
      type='button'
      class='btn-close'
      data-bs-dismiss='alert'
      aria-label='Close'
    ></button>
  </div>
{{/if}}

